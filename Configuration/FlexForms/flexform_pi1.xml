<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Function</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<switchableControllerActions>
						<TCEforms>
							<label>Select function</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">Display Map</numIndex>
										<numIndex index="1">Map->displaySingleMap</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">Display Youtube</numIndex>
										<numIndex index="1">Map->displayYoutube</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</switchableControllerActions>

					<settings.videoWidth>
						<TCEforms>
							<label>Video width</label>
							<config>
								<type>input</type>
								<size>3</size>
								<eval>int</eval>
								<default>420</default>
							</config>
						</TCEforms>
					</settings.videoWidth>

					<settings.videoHeight>
						<TCEforms>
							<label>Video height</label>
							<config>
								<type>input</type>
								<size>3</size>
								<eval>int</eval>
								<default>345</default>
							</config>
						</TCEforms>
					</settings.videoHeight>

					<settings.videoSource>
						<TCEforms>
							<label>Video source</label>
							<config>
								<type>input</type>
								<size>100</size>
								<max>200</max>
								<eval>trim</eval>
								<default>http://www.youtube.com/v/CLIPLiQDIk0</default>
							</config>
						</TCEforms>
					</settings.videoSource>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>